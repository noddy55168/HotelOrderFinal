
@{
    var httpContext = ViewContext.HttpContext;
    if (httpContext.Session.GetString("LayoutMessage") == "_LayoutMember")
    {
        Layout = "_LayoutMember";
    }
    else if ( httpContext.Session.GetString ( "LayoutMessage" ) == "_LayoutAdmin" )
    {
        Layout = "_LayoutAdmin";
    }
    else
    {
        Layout = "_Layout";
    }
}

